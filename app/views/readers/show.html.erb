<div class="container">
  <% @feeds.each do |feed| %>
    <% feed.entries.each do |entry| %>
    <div class="panel panel-default">
      <div class="panel-body">
        <%= link_to entry.title, entry , target: "_blank" %> - <i> published <%= time_ago_in_words(entry.published) %> ago.</i>
      </div>
    </div>
    <% end %>
  <% end %>
</div>
